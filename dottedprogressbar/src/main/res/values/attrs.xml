<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DottedProgressBar">
        <attr name="dpStyle" format="reference" />
        <attr name="dpBaseColor" format="color" />
        <attr name="dpArrowsColor" format="color" />
        <attr name="dpCenterColor" format="color" />
        <attr name="dpClockwiseDots" format="boolean" />
        <attr name="dpCounterClockwiseArrows" format="boolean" />
        <attr name="dpHideArrows" format="boolean" />
        <attr name="dpDotsCount" format="integer" />
        <attr name="dpMaxDotsSizePercent" format="integer" />
        <attr name="dpMinDotsSizePercent" format="integer" />
        <attr name="dpArrowWidthPercent" format="integer" />
        <attr name="dpArrowHourPaddingPercent" format="integer" />
        <attr name="dpArrowMinutePaddingPercent" format="integer" />
        <attr name="dpArrowHourSpeed" format="integer" />
        <attr name="dpArrowMinuteSpeed" format="integer" />
        <attr name="dpRotationSpeed" format="integer" />
    </declare-styleable>
</resources>